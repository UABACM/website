{
  "hosting": {
    "public": "main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/open-spots",
        "destination": "https://github.com/jaypyles/open-spots",
        "type": 301
      },
      {
        "source": "/uab-spots",
        "destination": "https://open-spots.vercel.app/",
        "type": 301
      }, {
        "source": "/discord",
        "destination": "https://discord.gg/3VrGNUunWa",
        "type": 302
      }, {
        "source": "/github",
        "destination": "https://github.com/UABACM",
        "type": 302
      }, {
        "source": "/instagram",
        "destination": "https://www.instagram.com/uabacm/",
        "type": 302
      }, {
        "source": "/assignment",
        "destination": "https://github.com/UABACM/first-contribution-uab",
        "type": 302
      }, {
        "source": "/chat",
        "destination": "https://chat.openai.com/g/g-gKvKQrVrw-gpt-for-uab",
        "type": 302
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(css|jpg|jpeg|gif|png|eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "404.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=960000"
          }
        ]
      }
    ]
  }
}
